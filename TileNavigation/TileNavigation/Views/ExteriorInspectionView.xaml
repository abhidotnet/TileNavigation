<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TileNavigation.Views.ExteriorInspectionView">
    <ContentPage.Content>
        <ScrollView>
        <StackLayout>
                <Label Text="Siding"/>
                <Label Text="Trim"/>
                <Label Text="Paint"/>
                <Label Text="Windows"/>
                <Label Text="Doors"/>
                <Label Text="Roof"/>
                <Label Text="Axles"/>
                <Label Text="Tire"/>
                <Label Text="Frame"/>
                <Button Text="Goto Interior" Command="{Binding GotoInterior}" />
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>