<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TileNavigation.Views.InteriorInspectionView">
    <ContentPage.Content>
        <ScrollView>
            <StackLayout>
                <Label Text="Floors"/>
                <Label Text="Ceiling"/>
                <Label Text="Walls"/>
                <Label Text="Doors / Hardware"/>
                <Label Text="Trim"/>
                <Label Text="Restroom"/>
                <Label Text="Electrical"/>
                <Label Text="Lights"/>
                <Label Text="HVAC"/>
                <Button Text="Goto Standard"  Command="{Binding GotoStandardCleaning}"/>
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>